#vfiles
library(ncdf4)
############################################################################################################################
################################################### 1978 ###################################################################
################################################### Extract u-component and calculate means by time ########################

u.wind.1978<-nc_open("C:/Users/Jon.Richar/Work/Projects/Bairdi_and_Opilio_SR_and SC_CS_dynamics/SR/DATA/wind_data/u/uwnd.10m.gauss.1978.nc")
u.wind.1978

lon.test<-ncvar_get(u.wind.1978,varid="lon")	#Degrees East--must subtract from 360 to get degrees west
lat.test<-ncvar_get(u.wind.1978,varid="lat")	#Degees North
time.test<-ncvar_get(u.wind.1978,varid="time")  #time is in hours, by 24 hr increment
wind.test<-ncvar_get(u.wind.1978,varid="uwnd")
lon.test
lat.test
time.test					#time is in hours, by 24 hr increment
wind.test


X <- ncvar_get(u.wind.1978,varid="uwnd")


May.78U<-X[101:108,17:18, 135:150]			 # Extracts data for 56.189-58.093N, 161.250-172.5W, May 15-May 30
May.78U
mean(May.78U)
mean.May.78U<-mean(May.78U)

June.78U<-X[101:108,17:18, 151:180]			 # Extracts data for 56.189-58.093N, 161.250-172.5W, June 1-June 30
June.78U
mean(June.78U)
mean.June.78U<-mean(June.78U)

July.78U<-X[101:108,17:18, 181:195]			 # Extracts data for 56.189-58.093N, 161.250-172.5W, July 1-July 15
July.78U
mean(July.78U)
mean.July.78U<-mean(July.78U)

May.June.78U<-X[101:108,17:18, 135:180]		 # Extracts data for 56.189-58.093N, 161.250-172.5W, May 15-June 30
May.June.78U
mean(May.June.78U)
mean.May.June.78U<-mean(May.June.78U)

JJ.78U<-X[101:108,17:18, 151:195]			 # Extracts data for 56.189-58.093N, 161.250-172.5W, June 1-July 15
JJ.78U
mean(JJ.78U)
mean.JJ.78U<-mean(JJ.78U)

May.July.78U <- X[101:108,17:18, 135:195]	       # Extracts data for 56.189-58.093N, 161.250-172.5W, May 15-July 15
May.July.78U
mean(May.July.78U)
mean.May.July.78U<-mean(May.July.78U)


