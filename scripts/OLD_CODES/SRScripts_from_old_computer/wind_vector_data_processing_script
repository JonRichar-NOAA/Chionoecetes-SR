############################### -45 degree component ###########################################


for(i in May.U){
D.neg.45.May=May.U * cos(-3*pi/12)+ May.V * sin(-3*pi/12)
}

for(i in June.U){
D.neg.45.June=June.U * cos(-3*pi/12)+ June.V * sin(-3*pi/12)
}

